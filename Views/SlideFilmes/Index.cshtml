
@{
    ViewData["Title"] = "Slide Filmes";
}
<div id="carouselExampleIndicators" class="carousel slide">
    <div class="carousel-indicators">
        <button type="button" data-bs-target="imgit push -u origin main" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button class="active btn btn bg-danger" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button class ="active btn btn bg-danger" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
        <button class="active btn btn bg-danger" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
        <button class="active btn btn bg-danger" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5"></button>
    </div>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="/img/266cbaf8-15c3-4a47-b81e-7776ae702ef8_m.jpg" class="d-block mx-auto" alt="...">
        </div>
        <div class="carousel-item">
            <img src="/img/680ac54e-014b-4ac7-ad05-db4cc955ebee_m.jpg" class="d-block mx-auto" alt="...">
        </div>
        <div class="carousel-item">
            <img src="/img/8995d442-c943-4945-af29-ca0c1c372a66_m.jpg" class="d-block mx-auto" alt="...">
        </div>
        <div class="carousel-item">
            <img src="/img/f254af36-b1b1-4077-94e2-bd68f1cc2f4e_m.jpg" class="d-block mx-auto" alt="...">
        </div>
        <div class="carousel-item">
            <img src="/img/fede9d6c-d7e3-41f6-a505-7d2ff4b3a555_m.jpg" class="d-block mx-auto" alt="...">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev" style="background-color:black">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next" style="background-color:black">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>